{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "allOf": [
        {
            "$ref": "#/definitions/MyObject"
        }
    ],
    "definitions": {
        "MyObject": {
            "additionalProperties": false,
            "description": "Some description here",
            "properties": {
                "stringValue": {
                    "type": "string",
                    "regexp": "/^hello$/i"
                }
            },
            "required": [
                "stringValue"
            ],
            "title": "Some title here",
            "type": "object"
        }
    }
}
